                    [key] => PVGEN-1346
                    [fields] => JiraRestApi\Issue\IssueField Object
                        (
                            [summary] => [Deployserver updaten] Cron für Datenbankstand-Transfer (Tarifverwaltung) Testserver
                            [progress] => Array
                                (
                                    [progress] => 0
                                    [total] => 0
                                )

                            [timeTracking] => 
                            [issuetype] => JiraRestApi\Issue\IssueType Object
                                (
                                    [self] => https://farpoint.get-ag.com/jira/rest/api/2/issuetype/10001
